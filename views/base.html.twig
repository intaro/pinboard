<!DOCTYPE html>
<html lang="en">
    <head>
        <title>{% block title %}Intaro Pinboard{% endblock %}</title>
        <link href="{{ base_url }}favicon.ico" rel="icon" type="image/x-icon" />
        <link href="{{ base_url }}css/bootstrap.min.css" rel="stylesheet" />
        <link href="{{ base_url }}css/application.css?2" rel="stylesheet" />
        <link href="{{ base_url }}css/tableStyle.css" rel="stylesheet" />
        <script src="{{ base_url }}js/amcharts/amcharts.js" type="text/javascript"></script>
    </head>
    <body>
        <div id="wrap">
            <div class="navbar navbar-inverse">
                <div class="navbar-inner">
                    <a class="brand" href="{{ path("index") }}"><img src="{{ base_url }}img/intaropinboard_logo.png" /></a>
                    {% include '_menu.html.twig' %}
                </div>
            </div>

            <div class="container">
              {% block content %}{% endblock %}
            </div>

            <div id="push"></div>
        </div>

        <footer class="footer">
          <div class="container">
            <p>Developed by <a href="http://intaro.ru" target="_blank">Intaro</a></p>
            <p>Code and documentation licensed under the MIT license.</p>
          </div>
        </footer>

        {% block javascripts %}
        <script src="{{ base_url }}js/jquery-1.9.1.min.js" type="text/javascript"></script>
        <script src="{{ base_url }}js/micro_tpl.js" type="text/javascript"></script>
        <script src="{{ base_url }}js/bootstrap.min.js" type="text/javascript"></script>
        {% endblock %}
    </body>
</html>